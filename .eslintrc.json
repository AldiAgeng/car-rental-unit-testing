{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "eslint linebreak-style": false,
    "camelcase": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-useless-constructor": "error"
  }
}
